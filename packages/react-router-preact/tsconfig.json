		"types": ["vite/client"],
		"moduleResolution": "nodenext",
		"module": "NodeNext",
		"verbatimModuleSyntax": true,
		"allowImportingTsExtensions": true,
		"rewriteRelativeImportExtensions": true,
		"strict": true,
